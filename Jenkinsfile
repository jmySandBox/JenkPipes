pipeline {
  agent any
  stages {
    stage('Bonjour') {
      steps {
        echo 'Hello World'
      }      
    }
    stage('La Deuxiemme Salut') {
      steps {
        echo 'Secondary Hello'
      }
    }
    stage('les trois') {
      steps {
        echo 'le trifecta'
      }
    }
    stage('le fin') {
      steps {
        echo 'I am Your Master - Thou Shalt Obey Me'
      }
    }
    stage('qui ma dire') {
      steps {
        echo 'You changed me'
      }
    }
  }
}
